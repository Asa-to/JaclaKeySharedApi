{
  "name": "jacla-key-shared-api",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "jest": "^26.6.3",
    "json5": "^2.2.0",
    "ts-dotenv": "^0.8.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts","js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/*.test.(ts|tsx|js)",
      "**/test/**/*.test.(ts|tsx|js)"
    ]
  },
  "scripts": {
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist/*",
    "debug": "yarn lint && tsc && yarn clean",
    "start": "ts-node src/index.ts",
    "test": "jest --silent false --verbose false"
  },
  "devDependencies": {
    "@types/figlet": "^1.2.1"
  }
}
